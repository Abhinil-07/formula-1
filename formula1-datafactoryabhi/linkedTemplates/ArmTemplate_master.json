{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "formula1-datafactoryabhi"
		},
		"ls_csvtoadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_csvtoadls'"
		},
		"ls_jsontoadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_jsontoadls'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0619-164539-nz0a11p3"
		},
		"ls_csvtoadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1dlabi2.dfs.core.windows.net/"
		},
		"ls_ingestcsvfromgithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Abhinil-07/"
		},
		"ls_jsonfromgithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Abhinil-07/"
		},
		"ls_jsontoadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1dlabi2.dfs.core.windows.net/"
		},
		"ls_lookupfromgithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Abhinil-07/formula-1/refs/heads/main/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "formula1-datafactoryabhi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_csvtoadls_accountKey": {
						"value": "[parameters('ls_csvtoadls_accountKey')]"
					},
					"ls_jsontoadls_accountKey": {
						"value": "[parameters('ls_jsontoadls_accountKey')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"ls_csvtoadls_properties_typeProperties_url": {
						"value": "[parameters('ls_csvtoadls_properties_typeProperties_url')]"
					},
					"ls_ingestcsvfromgithub_properties_typeProperties_url": {
						"value": "[parameters('ls_ingestcsvfromgithub_properties_typeProperties_url')]"
					},
					"ls_jsonfromgithub_properties_typeProperties_url": {
						"value": "[parameters('ls_jsonfromgithub_properties_typeProperties_url')]"
					},
					"ls_jsontoadls_properties_typeProperties_url": {
						"value": "[parameters('ls_jsontoadls_properties_typeProperties_url')]"
					},
					"ls_lookupfromgithub_properties_typeProperties_url": {
						"value": "[parameters('ls_lookupfromgithub_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}